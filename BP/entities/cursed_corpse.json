				"is_pushable": false,
				"is_pushable_by_piston": true
			},
			"minecraft:conditional_bandwidth_optimization": {}
		},
		"events": {
			"rev:death": {
				"run_command": {
					"command": [
						"structure load mystructure:loot_base ~~~",
						"particle rev:red_dusk_ritual ~ ~ ~"
					]
				}
			},
			"rev:summon_mobs": {
				"randomize": [
					{
						"weight": 2,
						"run_command": {
							"command": [
								"particle rev:red_dusk_ritual ~ ~ ~",
								"summon zombie ^2 ^1 ^",
								"summon zombie ^-2 ^1 ^"
							]
						}
					},
					{
						"weight": 1,
						"run_command": {
							"command": [
								"particle rev:red_dusk_ritual ~ ~ ~",
								"summon skeleton ^2 ^1 ^",
								"summon skeleton ^-2 ^1 ^"
							]
						}
					}
				]
			},
			"minecraft:entity_spawned": {
				"randomize": [
					{
						"weight": 1,
						"randomize": [
							{
								"weight": 80,
								"filters": {
									"all_of": [
										{
											"test": "is_daytime",
											"value": false
										},
										{
											"test": "is_snow_covered",
											"value": true
										},
										{
											"test": "is_underground",
											"value": false
										}
									]
								},
								"remove": {},
								"add": {
									"component_groups": [
										"rev:cursed_corpse_stray_jockey",
										"rev:cursed_corpse_neutral"
									]
								}
							},
							{
								"weight": 80,
								"filters": {
									"test": "is_biome",
									"value": "the_nether"
								},
								"remove": {},
								"add": {
									"component_groups": [
										"rev:cursed_corpse_wither_jockey",
										"rev:cursed_corpse_neutral"
									]
								}
							},
							{
								"weight": 20,
								"filters": {
									"any_of": [
										{
											"test": "is_daytime",
											"value": false
										},
										{
											"test": "is_underground",
											"value": true
										}
									]
								},
								"remove": {},
								"add": {
									"component_groups": [
										"rev:cursed_corpse_jockey",
										"rev:cursed_corpse_neutral"
									]
								}
							}
						]
					},
					{
						"weight": 99,
						"remove": {},
						"add": {
							"component_groups": [
								"rev:cursed_corpse_neutral"
							]
						}
					}
				]
			},
			"minecraft:become_hostile": {
				"remove": {
					"component_groups": [
						"rev:cursed_corpse_neutral"
					]
				},
				"add": {
					"component_groups": [
						"rev:cursed_corpse_hostile"
					]
				}
			},
			"minecraft:become_neutral": {
				"remove": {
					"component_groups": [
						"rev:cursed_corpse_hostile"
					]
				},
				"add": {
					"component_groups": [
						"rev:cursed_corpse_neutral"
					]
				}
			},
			"minecraft:become_angry": {
				"add": {
					"component_groups": [
						"rev:cursed_corpse_angry"
					]
				}
			},
			"minecraft:become_calm": {
				"remove": {
					"component_groups": [
						"rev:cursed_corpse_angry"
					]
				}
			}
		}
	}
}