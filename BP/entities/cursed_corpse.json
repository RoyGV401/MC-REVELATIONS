{
	"format_version": "1.16.100",
	"minecraft:entity": {
		"description": {
			"identifier": "rev:cursed_corpse",
			"is_spawnable": true,
			"is_summonable": true,
			"is_experimental": false,
			"runtime_identifier": "minecraft:spider"
		},
		"components": {
			"minecraft:behavior.nearest_attackable_target": {
				"priority": 3,
				"must_see": true,
				"entity_types": [
					{
						"filters": {
							"any_of": [
								{
									"test": "is_family",
									"subject": "other",
									"value": "player"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "snowgolem"
								},
								{
									"test": "is_family",
									"subject": "other",
									"value": "irongolem"
								}
							]
						},
						"max_dist": 30
					}
				]
			},
			"minecraft:timer": {
				"looping": true,
				"time": 5,
				"time_down_event": {
					"filters": {
						"test": "has_target",
						"subject": "self",
						"value": true
					},
					"event": "rev:summon_mobs"
				}
			},
			"minecraft:experience_reward": {
				"on_death": "query.last_hit_by_player?20:0"
			},
			"minecraft:nameable": {},
			"minecraft:type_family": {
				"family": [
					"undead",
					"mob",
					"monster"
				]
			},
			"minecraft:breathable": {
				"total_supply": 15,
				"suffocate_time": 0
			},
			"minecraft:loot": {
				"table": "loot_tables/entities/cursed_corpse.json"
			},
			"minecraft:collision_box": {
				"width": 0.8,
				"height": 0.9
			},
			"minecraft:knockback_resistance": {
				"value": 2,
				"max": 2
			},
			"minecraft:health": {
				"value": 40,
				"max": 40
			},
			"minecraft:hurt_on_condition": {
				"damage_conditions": [
					{
						"filters": {
							"test": "in_lava",
							"subject": "self",
							"operator": "==",
							"value": true
						},
						"cause": "lava",
						"damage_per_tick": 4
					}
				]
			},
			"minecraft:navigation.climb": {
				"can_path_over_water": true
			},
			"minecraft:jump.static": {},
			"minecraft:can_climb": {},
